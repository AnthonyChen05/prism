# Application
NODE_ENV=development
PORT=3000

# Landing page â€” folder name of the module to serve at /
# Change this to any module folder name (e.g. "home", "start")
LANDING_MODULE=dashboard

# JWT
JWT_SECRET=change-me-to-a-long-random-secret-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# PostgreSQL (matches docker-compose service)
DATABASE_URL=postgresql://prism:prism_password@postgres:5432/prism_db

# Redis (matches docker-compose service)
REDIS_HOST=redis
REDIS_PORT=6379

# BullMQ queue names
SCHEDULER_QUEUE=prism-scheduler
TIMER_QUEUE=prism-timers
NOTIFICATION_QUEUE=prism-notifications
